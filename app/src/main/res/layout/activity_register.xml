<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterActivity">

    <Button
        android:id="@+id/button_Register_Done"
        android:layout_width="246dp"
        android:layout_height="82dp"
        android:layout_marginBottom="105dp"
        android:text="Done"
        app:backgroundTint="#FFB48569"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editText_Register_password"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="23dp"
        android:ems="10"
        android:inputType="textPersonName|textPassword"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textViewError_Register_username" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginEnd="29dp"
        android:fontFamily="@font/alegreya_sc"
        android:text="Password"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText_Register_password"
        app:layout_constraintEnd_toStartOf="@+id/editText_Register_password"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editText_Register_username"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="81dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/alegreya_sc"
        android:text="User Name"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/editText_Register_username"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewError_Register_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="12dp"
        android:text=" "
        android:textColor="#BF0303"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="@+id/editText_Register_username"
        app:layout_constraintTop_toBottomOf="@+id/editText_Register_username" />

    <TextView
        android:id="@+id/textViewError_Register_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:text=" "
        android:textColor="#BF0303"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="@+id/editText_Register_password"
        app:layout_constraintTop_toBottomOf="@+id/editText_Register_password" />

    <EditText
        android:id="@+id/editText_Register_phone"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="67dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="67dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/editText_Register_password"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/editText_Register_password" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="51dp"
        android:fontFamily="@font/alegreya_sc"
        android:text="Birthday"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewError_Register_email" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/alegreya_sc"
        android:text="Phone"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewError_Register_password" />

    <EditText
        android:id="@+id/editTextDateBirthday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Select Date"
        android:textAlignment="center"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textViewError_Register_email"
        android:layout_width="52dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="103dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="212dp"
        android:text=" "
        android:textColor="#BF0303"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/button_Register_Done"
        app:layout_constraintEnd_toEndOf="@+id/editTextDateBirthday"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewError_Register_password" />

    <EditText
        android:id="@+id/editText_Register_email"
        android:layout_width="337dp"
        android:layout_height="45dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="23dp"
        android:ems="10"
        android:inputType="textPersonName|textEmailAddress"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/textViewError_Register_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewError_Register_phone" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:fontFamily="@font/alegreya_sc"
        android:text="Email"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editText_Register_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editText_Register_email" />

    <TextView
        android:id="@+id/textViewError_Register_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="29dp"
        android:text=" "
        android:textColor="#BF0303"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="@+id/editText_Register_phone"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

</androidx.constraintlayout.widget.ConstraintLayout>